{
    "name": "newage/generators",
    "type": "library",
    "description": "Code generators for zend framework 2 on console.",
    "keywords": [
        "newage",
        "generators"
    ],
    "homepage": "https://github.com/newage/generators",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Leontiev",
            "email": "vadim.leontiev@gmail.com",
            "homepage": "https://github.com/newage",
            "role": "Developer"
        }
    ],
    "config": {
        "bin-dir": "bin/"
    },
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Newage\\Generators\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Newage\\Generators\\": "tests"
        }
    },
    "scripts": {
        "test": "php bin/phpunit -c phpunit.xml.dist",
        "cs": "php bin/phpcs src --standard=PSR2 --colors",
        "cs-fix": "php bin/phpcbf src --standard=PSR2 --colors"
    }
}
